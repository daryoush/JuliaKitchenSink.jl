<span class="n">nu</span>  <span class="o">=</span> <span class="n">OxfordH</span><span class="p">[</span><span class="s1">'nu_est'</span><span class="p">][</span><span class="mi">0</span><span class="p">]</span> <span class="c1"># Vol of vol estimate for SPX</span>
