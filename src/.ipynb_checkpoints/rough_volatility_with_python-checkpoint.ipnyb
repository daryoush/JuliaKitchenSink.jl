<div class=" highlight hl-ipython2"><pre><span></span><span class="n">h</span> <span class="o">=</span> <span class="nb">list</span><span class="p">()</span>
<span class="n">nu</span> <span class="o">=</span> <span class="nb">list</span><span class="p">()</span>

<span class="k">for</span> <span class="n">col</span> <span class="ow">in</span> <span class="n">rv1</span><span class="o">.</span><span class="n">columns</span><span class="p">:</span>
